<%= simple_form_for @experience do |f| %>
  <%= f.input :title %>
  <%= f.input :category, as: :select, collection: Experience.categories %>
  <%= f.input :description %>
  <%= f.input :address %>
  <%= f.input :country, as: :string %>
  <%= f.input :photos, as: :attachinary %>
  <%= f.submit %>
<% end %>
