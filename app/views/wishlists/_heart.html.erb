<span style="font-size: 18px;";>Bookmark </span>
<% if wished %>

  <%= link_to wishlist_path(wishlist), method: :delete, remote: true do %>
    <i class="fa fa-star"></i>
  <% end %>

<% else %>

  <%= link_to experience_wishlists_path(experience), method: :post, remote: true do %>
    <i class="fa fa-star-o"></i>
  <% end %>

<% end %>
